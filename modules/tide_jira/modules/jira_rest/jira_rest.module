<?php

/**
 * @file
 * Contains jira_rest.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function jira_rest_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the jira_rest module,available on the admin help
    // page (admin/help)
    // * The second example text will be.
    case 'help.page.jira_rest':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Providing methods to modify JIRA issues out of Drupal via REST.') . '</p>';
      return $output;

    default:
  }
}
