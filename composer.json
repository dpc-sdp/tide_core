        "drupal/ckeditor_liststyle": "^1.4",
        "drupal/ckeditor_scayt": "^1.3",
        "drupal/clamav": "^2.0",
        "drupal/config_ignore": "^2.1",
        "drupal/config_perms": "^2.0",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/core": "9.4.x",
        "drupal/ctools": "^3.11",
        "drupal/diff": "^1.0-RC2",
