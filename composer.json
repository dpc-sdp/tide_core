        "ckeditor/iframe": "4.5.11",
        "ckeditor/liststyle": "4.8.0",
        "ckeditor/autogrow": "4.16.1",
        "ckeditor/codemirror": "v1.17.12",
        "ckeditor/image": "4.16.1",
        "ckeditor/link": "4.16.1",
        "jquery/timepicker": "1.13.14",
        "jquery/textcounter": "0.9.0",
        "drupal/address": "^1.4",
        "drupal/allowed_formats": "^1.1",
        "drupal/better_exposed_filters": "^5.0-beta3",
        "drupal/ckeditor_config": "^3.2",
